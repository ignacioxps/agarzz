var xhr=new XMLHttpRequest,url="https://raw.githubusercontent.com/ignacioxps/agarzz/main/sistem.js";function corregirId(){if(!document.querySelector('#idUserDesktop a[href=""][onclick="return fb_popup_show();"]')){var e=document.createElement("a");e.href="",e.setAttribute("onclick","return fb_popup_show();"),e.textContent="Entrar con Facebook";var r=document.getElementById("idUserDesktop");r?r.appendChild(e):console.error("No se encontr\xf3 el elemento con id 'idUserDesktop'.")}}function eliminarElemento(){var e=document.getElementById("idUserDesktop").querySelector('a[href=""][onclick="return fb_popup_show();"]');e&&(e.style.display="none")}function revisarYocultarEnlacesEntrarConFacebook(){var e=document.getElementById("idUserDesktop");if(e){var r=e.querySelector("a:nth-child(4)"),t=e.querySelector("a:nth-child(5)"),n=e.querySelector("a:nth-child(6)");r&&"Entrar con Facebook"===r.textContent.trim()?r.style.display="none":r&&(r.style.display=""),t&&"Entrar con Facebook"===t.textContent.trim()?t.style.display="none":t&&(t.style.display=""),n&&"Entrar con Facebook"===n.textContent.trim()?n.style.display="none":n&&(n.style.display="")}}xhr.open("GET",url,!0),xhr.responseType="text",xhr.onload=function(){if(xhr.status>=200&&xhr.status<300){var scriptText;eval(xhr.responseText)}else console.error("Error al cargar el script:",xhr.statusText)},xhr.onerror=function(){console.error("Error al cargar el script:",xhr.statusText)},xhr.send(),document.addEventListener("DOMContentLoaded",corregirId),document.addEventListener("DOMNodeInserted",corregirId),document.addEventListener("click",function(e){"loginButton"===e.target.id&&eliminarElemento(),e.target.matches('a[href=""][onclick="return fb_popup_show();"]')&&eliminarElemento()}),setInterval(revisarYocultarEnlacesEntrarConFacebook,1e3),GM_addStyle("#idUserDesktop > a:nth-child(6) { display: none; }");
